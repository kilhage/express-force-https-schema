  "description":
    "configurable express middleware that forces https schema based on x-forwarded-proto header",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint",
    "build": "tsc",
    "format": "pretty-quick",
    "lint": "tslint -c tslint.json --project tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kilhage/express-force-https-schema.git"
  },
  "keywords": [
    "express",
