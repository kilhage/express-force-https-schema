    "aws",
    "heroku",
    "x-forwarded-proto",
    "force"
  ],
  "author": "emilkilhage@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kilhage/express-force-https-schema/issues"
  },
  "homepage": "https://github.com/kilhage/express-force-https-schema#readme",
  "dependencies": {
    "@types/express": "^4.11.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.1"
  }
}
